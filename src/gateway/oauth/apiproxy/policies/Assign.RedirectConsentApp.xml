<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Assign.RedirectConsentApp">
    <DisplayName>Assign.RedirectConsentApp</DisplayName>
    <Properties/>
    <Remove>
        <Payload/>
    </Remove>
    <Set>
        <StatusCode>302</StatusCode>
        <Headers>
            <Header name="Location">{oauth_consent_app_redirect}</Header>
            <Header name="Access-Control-Allow-Origin">*</Header>
            <Header name="Access-Control-Allow-Methods">POST, GET, OPTIONS</Header>
            <Header name="Access-Control-Allow-Headers">Accept, Content-Type, connection, content-length, Authorization, Location</Header>
            <Header name="Access-Control-Allow-Credentials">true</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>